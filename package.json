    "react-countup": "^6.5.3",
    "react-dom": "^18",
    "react-scroll": "^1.9.0",
    "react-syntax-highlighter": "^15.6.1",
    "scrollcue": "^2.0.0",
    "sharp": "^0.33.5",
    "swiper": "^11.1.15",
    "typewriter-effect": "^2.21.0"
  },
  "devDependencies": {
